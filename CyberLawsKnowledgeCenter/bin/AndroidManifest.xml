<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cyberlawsknowledgecenter"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="19" />
    <uses-permission android:name="android.permission.CALL_PHONE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.cyberlawsknowledgecenter.HomeActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.cyberlawsknowledgecenter.AboutUs"
            android:label="@string/title_activity_about_us" >
        </activity>
        <activity
            android:name="com.example.cyberlawsknowledgecenter.OurFaculty"
            android:label="@string/title_activity_our_faculty" >
        </activity>
        <activity
            android:name="com.example.cyberlawsknowledgecenter.ContactUs"
            android:label="@string/title_activity_contact_us" >
        </activity>
        <activity
            android:name="com.example.cyberlawsknowledgecenter.ItLawCourse"
            android:label="@string/title_activity_main_courses" >
        </activity>
        <activity
            android:name="com.example.cyberlawsknowledgecenter.CoursesHome"
            android:label="@string/title_activity_courses_home" >
        </activity>
        <activity
            android:name="com.example.cyberlawsknowledgecenter.CyberLawCourse"
            android:label="@string/title_activity_cyber_law_course" >
        </activity>
        <activity
            android:name="com.example.cyberlawsknowledgecenter.IprLawCourse"
            android:label="@string/title_activity_ipr_law_course" >
        </activity>
        <activity
            android:name="com.example.cyberlawsknowledgecenter.CDDCPActivity"
            android:label="@string/title_activity_cddcp" >
        </activity>
    </application>

</manifest>
